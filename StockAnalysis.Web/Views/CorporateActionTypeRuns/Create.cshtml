                <span asp-validation-for="InsertType" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="FilePath" class="control-label"></label>
                <input asp-for="FilePath" class="form-control" type="file" accept=".pdf,.csv" name="File" />
